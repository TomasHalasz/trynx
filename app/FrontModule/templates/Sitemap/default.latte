    </url>
    <url>
	<loc>https://klienti.cz/clanek/helpdesk</loc>
      <changefreq>weekly</changefreq>
      <priority>1.00</priority>
    </url>
    <url>
	<loc>https://klienti.cz/clanek/klienti</loc>
      <changefreq>weekly</changefreq>
      <priority>1.00</priority>
    </url>	
{foreach $articles as $one}
    <url>
	<loc>https://klienti.cz{link :Front:BlogDetail:default title => $one->name, id => $one->id, blog_categories_id => NULL}</loc>
      <changefreq>weekly</changefreq>
      <priority>1.00</priority>
    </url>
{/foreach}    
{foreach $categories as $one}
    <url>
	{capture $name}{$one->name|webalize}{/capture}
    {*$name = $name*}
	<loc>https://klienti.cz{link :Front:Blog:default  name => $name, blog_categories_id => $one->id}</loc>
      <changefreq>weekly</changefreq>
      <priority>1.00</priority>
    </url>
{/foreach}    
{foreach $tags as $one}
    <url>
	{capture $name}{$one->name|webalize}{/capture}
        {*$name = $name*}
        <loc>https://klienti.cz{link :Front:Blog:tags  name => $name, blog_tags_id => $one->id}</loc>
      <changefreq>weekly</changefreq>
      <priority>1.00</priority>
    </url>
{/foreach}    
</urlset>










