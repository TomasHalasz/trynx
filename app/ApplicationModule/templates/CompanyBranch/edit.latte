							<div class="form-group form-group-sm">
								<div class="col-sm-4 ">
									<div class="checkbox checkbox-inline checkbox-default">
										{input eet_active: class=>"styled"}<label n:name="$form[eet_active]">{$form[eet_active]->caption}</label>
									</div>
								</div>
							</div>
						</div>
						<div class="row">
							<div class="form-group form-group-sm">
								<div class="col-sm-4 ">
									<div class="checkbox checkbox-inline checkbox-default">
										{input eet_test: class=>"styled"}<label n:name="$form[eet_test]">{$form[eet_test]->caption}</label>
