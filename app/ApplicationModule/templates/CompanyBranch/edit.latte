{block #content}


{if $modal}
    <div class="containerMyModal">
{else}
    <div class="containerMy">    
{/if}
    <div class="row mySeparator">
    </div>
    <div class="row mySeparator">
    </div>
	<div class="panel panel-primary">

	    <div class="panel-heading">
				{var $formName = 'Pobočka'}
				{include '../../../templates/panelheading.latte' formName => $formName}
		</div>
	    <div class="panel-body">

			<ul id="myTabs" class="nav nav-tabs" role="tablist">
				<li role="presentation" class="{if $activeTab == 1}active{/if}"><a href="#tab1" aria-controls="tab1" role="tab" data-toggle="tab">{_Karta}</a></li>
				<li role="presentation" class="{if $activeTab == 2}active{/if}"><a href="#tab2" aria-controls="tab2" role="tab" data-toggle="tab">{_Pracovníci}</a></li>
			</ul>
			<div class="col-sm-12">
			<div class="tab-content">
			<div role="tabpanel" id="tab1" class="tab-pane active">
				{form edit}
				{control $form errors}
						<div class="row">
							<div class="col-md-3">
								<div class="form-group">
									{label name class=>"control-label"/}
									{input name class=>"form-control input-sm"}
								</div>
							</div>
							<div class="col-md-3">
								<div class="form-group">
									{label b_name class=>"control-label"/}
									{input b_name class=>"form-control input-sm"}
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-md-3">
							<div class="form-group">
								{label b_street class=>"control-label"/}
								{input b_street class=>"form-control input-sm"}
							</div>
							</div>
							<div class="col-md-3">
								<div class="form-group">
									{label b_city class=>"control-label"/}
									{input b_city class=>"form-control input-sm"}
