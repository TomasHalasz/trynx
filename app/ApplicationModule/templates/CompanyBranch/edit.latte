									{label cl_storage_id class=>"control-label"/}
									{input cl_storage_id class=>"form-control input-sm chzn-select"}
								</div>
							</div>
							<div class="col-md-3">
								<div class="form-group">
									{label cl_partners_book_id class=>"control-label"/}
									{input cl_partners_book_id class=>"form-control input-sm chzn-select"}
								</div>
							</div>
							<div class="col-md-3">
								{snippet branchKey}
								<div class="form-group">
									<label class="control-label" for="branch_key"> {_Klíč_pro_externí_spojení}</label>
									<input name="branch_key" id="branch_key" maxlength="64" size="30" placeholder={_Klíč_pro_externí_spojení}  type="text" readonly="readonly" value="{$data['branch_key']}" class="form-control input-sm">
								</div>
								{/snippet}
							</div>
							<div class="col-md-3">
								<div class="form-group">
									<br>
