									{label b_phone class=>"control-label"/}
									{input b_phone class=>"form-control input-sm"}
								</div>
							</div>
							<div class="col-md-3">
								<div class="form-group">
									{label b_www class=>"control-label"/}
									{input b_www class=>"form-control input-sm"}
								</div>
							</div>
						</div>
